{
    "name": "Satoko Errors",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
      "webRequest",
      "<all_urls>"
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "web_accessible_resources": [
      "*.html",
      "images/*.png"
    ],
    "icons": {"48": "images/icon.png"}
  }
  